cat: ("Special" ; "Physical" ; "Status"), -space.

power: ([N]+ ; em), -space.

acc: ([N]+ ; em), -space.

pp: [N]+, -space.

-tab: -#9.
-newline: (-#d?, #a).
-space: -#20.
-em: #2014.
-text: ~[#9;#d;#a]+.
